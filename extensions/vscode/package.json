{
  "private": true,
  "publisher": "azdavis",
  "name": "millet-ls",
  "displayName": "Millet LS",
  "version": "0.1.0",
  "description": "A language server for Standard ML",
  "author": "Ariel Davis <ariel.z.davis@icloud.com>",
  "main": "./out/main.js",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/vscode": "^1.46.0",
    "typescript": "^3.9.5"
  },
  "engines": {
    "vscode": "^1.46.1"
  },
  "activationEvents": [
    "onLanguage:sml"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sml",
        "extensions": [
          ".sml",
          ".sig",
          ".fun"
        ],
        "aliases": [
          "Standard ML"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sml",
        "scopeName": "source.sml",
        "path": "./syntaxes/sml.tmLanguage.json"
      }
    ]
  }
}
